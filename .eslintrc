{
  "extends": ["airbnb"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "react/function-component-definition": "off",
    "react/prop-types": "off",
    "import/extensions": "off",
    "react/jsx-filename-extension": [0, { "allow": "as-needed" }]
  }
}